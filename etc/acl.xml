<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="Magenest_Movie::config_movie"
                                      title="Movie Section" />
                        </resource>
                    </resource>
                </resource>
                <resource id="Magenest_Movie::movie" title="Movie">
                    <resource id="Magenest_Movie::movie_table" title="All Movies">
                    </resource>
                    <resource id="Magenest_Movie::actor" title="All Actors">
                    </resource>
                    <resource id="Magenest_Movie::director" title="All Directors">
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>